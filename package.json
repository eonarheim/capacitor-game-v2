    "@capacitor/core": "latest",
    "@capacitor/splash-screen": "latest",
    "excalibur": "0.25.3"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "@parcel/packager-raw-url": "^2.3.2",
    "@parcel/transformer-webmanifest": "^2.3.2",
    "parcel": "2.3.2"
  },
  "author": "",
  "license": "ISC"
}
